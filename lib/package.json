{
  "name": "rollup-lib",
  "description": "Web Library",
  "version": "1.0.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn build:library",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "build:library": "rm -rf /dist && rollup -c",
    "build:storybook": "rm -rf /build && build-storybook && mv storybook-static build",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "pipeline:build": "yarn build",
    "pipeline:test:unit": "yarn test --coverage",
    "generate": "plop --plopfile generators/plopfile.js"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@faker-js/faker": "^8.4.1",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@rollup/plugin-url": "^6.1.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^9.5.1",
    "@storybook/addon-a11y": "^6.1.15",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack4": "^6.5.13",
    "@storybook/client-api": "^7.1.1",
    "@storybook/manager-webpack4": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/rollup": "^8.1.0",
    "@svgr/webpack": "5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "babel-loader": "^8.3.0",
    "babel-plugin-styled-components": "^2.1.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "jest-styled-components": "^7.1.1",
    "lint-staged": "^13.1.2",
    "plop": "^3.1.2",
    "postcss": "^8.4.21",
    "prettier": "2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-responsive": "^10.0.0",
    "rollup": "^2.53.1",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-iconfont": "^1.0.3",
    "rollup-plugin-import-css": "^3.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-static-files": "^0.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^19.0.5",
    "styled-components": "^5.3.9",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write --ignore-unknown",
    "src/**/*.{js,jsx,ts,tsx}": "yarn lint:fix"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.9"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.20.5",
    "@types/dompurify": "^3.0.5",
    "dompurify": "^3.1.5"
  }
}